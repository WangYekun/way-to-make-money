/**
 * 根据批量组织节点获取成员集合
 *
 * @param orgId 组织节点
 * @param memberId 成员id
 * @return List<MemberDeptResp>
 */
List<MemberDeptResp> getUserIdsByOrgIds(@Param("") Integer orgId, @Param("") Integer );

/**
 * 根据批量组织节点获取成员集合
 *
 * @param orgId 组织节点
 * @return List<Integer>
 */
List<Integer> getUserIdsById(@Param("") Integer );


/**
 * 根据获取id获取成员数据和公司数据
 *
 * @param userId 用户id
 * @return List<MemberAndCompanyVO> 成员数据和公司数据数据集合
 */
List<MemberAndCompany> getMemberAndCompanyByUserId(@Param("") Integer );
